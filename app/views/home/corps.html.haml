= cache(cache_key) do
  .row
    .span6#kill_box
      %h4 Top bounty this month
      - top_corp_bounty_this_month.each do |top|
        .span1#kill
          %b
            = isk_short top["sum"]
          = corp_image_link(top["_id"])
          %br
          %b
            = corp_name_link(top["_id"])
    .span6#kill_box
      %h4 Top tax this month
      - top_tax_this_month.each do |top|
        .span1#kill
          %b
            = isk_short top["sum"]
          = corp_image_link(top["_id"])
          %br
          %b
            = corp_name_link(top["_id"])
  %br
  .row
    .span6#kill_box
      %h4 Top bounty last 10 days
      - top_corp_bounty_10days.each do |top|
        .span1#kill
          %b
            = isk_short top["sum"]
          = corp_image_link(top["_id"])
          %br
          %b
            = corp_name_link(top["_id"])
    .span6#kill_box
      %h4 Top tax last 10 days
      - top_tax_10days.each do |top|
        .span1#kill
          %b
            = isk_short top["sum"]
          = corp_image_link(top["_id"])
          %br
          %b
            = corp_name_link(top["_id"])
