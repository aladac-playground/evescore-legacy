.row
  .span6#kill_box
    %h4 Top bounty last 10 days
    - top_bounty_10days.each do |top|
      .span1#kill
        %b
          = isk_short top["sum"]
        %img.img-rounded{ :src => character_image(top["_id"])}
        %br
        %b
          = link_to character_name(top["_id"]), "/character/#{top["_id"]}"
  .span6#kill_box
    %h4 Highest bounty last 10 days
    - highest_bounty_10days.each do |top|
      .span1#kill
        %b
          = isk_short(top[:bounty])
        %img.img-rounded{ :src => rat_image(top[:rat_id])}
        %br
        %b
          = link_to rat_name(top[:rat_id]), "/kills/log?rat_id=#{top[:rat_id]}"

%br
