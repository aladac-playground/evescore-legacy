.row
  .span6#kill_box
    %h4 Top bounty last 10 days
    - top_bounty_10days.each do |top|
      .span1#kill
        %b
          = isk_short top["sum"]
        = character_image_link(top["_id"])
        %br
        %b
          = character_name_link(top["_id"])
  .span6#kill_box
    %h4 Highest bounty last 10 days
    - highest_bounty_10days.each do |top|
      .span1#kill
        %b
          = isk_short(top[:bounty])
        = rat_image_link top[:rat_id]
        %br
        %b
          = rat_name_link top[:rat_id]

%br
